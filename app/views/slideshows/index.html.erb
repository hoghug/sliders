<% @slideshows.each do |slideshow| %>
	<%= render 'slideshow', slideshow: slideshow %>
<% end %>

<% if user_signed_in? %>
	<%= simple_form_for @slideshow do |f| %>
		<h3>New Slideshow</h3>
		<%= f.input :title, label: false, placeholder: 'Title' %>
		<%= f.button :submit, class: 'btn-primary' %>
	<% end %>
<% end %>
